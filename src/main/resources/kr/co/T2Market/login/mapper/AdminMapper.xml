<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="kr.co.T2Market.login.mapper.AdminMapper">

	<select id="login" resultType="kr.co.T2Market.login.domain.AdminVO">
		select * from admin_info
		where admin_id = #{admin_id} and pass = #{pass}
	</select>

</mapper>